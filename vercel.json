{
  "version": 2,
  "name": "medi-ai",
  "builds": [
    {
      "src": "web/Backend/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "web/Backend/api/index.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "web/Backend/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "web/Frontend/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "web/Backend/api/index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "/web/Backend/api/index.js"
    }
  ]
}
